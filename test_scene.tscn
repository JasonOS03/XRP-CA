[node name="Label3D" type="Label3D" parent="GoalTextParent/Goal2Text/MeshInstance3D"]
transform = Transform3D(0.15, 0, 0, 0, 0.7, 0, 0, 0, 1, 0, 0, 0)
text = "● TU provides community garden spaces on its campuses that foster organic food
growing, education in sustainable agriculture, and community well‑being
These gardens also offer hands‑on learning and access to fresh produce for
students and local partners through engagement initiatives"

[node name="Goal3Text" type="Node3D" parent="GoalTextParent"]
transform = Transform3D(0.919828, 0, 0.392321, 0, 1, 0, -0.392321, 0, 0.919828, -2.40736, 1.72698, -3.47882)

[node name="MeshInstance3D" type="MeshInstance3D" parent="GoalTextParent/Goal3Text"]
